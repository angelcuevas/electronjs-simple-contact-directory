{
  "name": "e-app",
  "version": "0.1.0",
  "private": true,
  "main": "public/main.js",
  "description": "just an electron app",
  "author": "Cuevas",
  "homepage": ".",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "bootstrap": "^5.2.2",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "sequelize": "^6.25.7",
    "sqlite3": "^5.1.2",
    "sweetalert2": "^11.6.10",
    "typescript": "^4.9.3",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-serve": "concurrently -k \"npm start\" \"npm run electron-start\" ",
    "electron-build": "npm run build  && electron-builder -c.extraMetadata.main=build/main.js",
    "electron-start": "wait-on tcp:3000 && nodemon --watch . --exec \"electron .\"",
    "enode": "concurrently -k \"npm start\" \"npm run electron-start\" "
  },
  "build": {
    "extends": null,
    "appId": "com.example-electron-cra",
    "extraResources": [
      "./extraResources/**"
    ],
    "files": [
      "dist/**/*",
      "build/**/*",
      "database/**/*",
      "node_modules/**/*",
      "package.json",
      "test.db"
    ],
    "directories": {}
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^21.3.0",
    "electron-builder": "^23.6.0"
  }
}
